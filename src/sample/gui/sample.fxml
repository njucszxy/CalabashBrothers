<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.gui.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints maxWidth="144.0" minWidth="91.0" prefWidth="120.0" />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <TextArea fx:id="textArea" prefHeight="100.0" prefWidth="600.0" GridPane.rowIndex="2" />
      <Pane fx:id="pane" prefHeight="600.0" prefWidth="600.0" GridPane.rowIndex="1">
         <children>
            <Canvas fx:id="layer1" height="600.0" width="600.0" />
            <Canvas fx:id="layer2" height="600.0" width="600.0" />
         </children>
      </Pane>
      <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="300.0" prefWidth="200.0">
               <children>
                  <RadioButton fx:id="choiceMO" layoutY="43.0" mnemonicParsing="false" text="MO" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="150.0" />
                  <RadioButton fx:id="choiceCB" layoutY="129.0" mnemonicParsing="false" text="CB" AnchorPane.bottomAnchor="150.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0" />
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="200.0">
               <children>
                  <ChoiceBox fx:id="choiceBox" layoutY="99.0" prefWidth="200.0" AnchorPane.bottomAnchor="195.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0" />
                  <Button fx:id="buttonNew" layoutX="4.0" layoutY="123.0" mnemonicParsing="false" onMouseClicked="#NewGame" prefWidth="200.0" text="New" AnchorPane.bottomAnchor="122.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="123.0" />
                  <Button fx:id="buttonLoad" layoutX="4.0" layoutY="203.0" mnemonicParsing="false" onMouseDragged="#LoadGame" text="Load" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="200.0" />
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </children>
</GridPane>
